{
  "id": "sheet-locker",
  "title": "Sheet Locker",
  "description": "Lock and unlock all character sheets at once as the GM with just one click! Protect your players from accidental changes, or prevent cheating.",
  "version": "1.0.0",
  "url": "https://github.com/coffiarts/FoundryVTT-sheet-locker",
  "manifest": "https://github.com/coffiarts/FoundryVTT-sheet-locker/releases/download/latest/module.json",
  "download": "https://github.com/coffiarts/FoundryVTT-sheet-locker/releases/download/latest/module.zip",
  "readme": "https://github.com/coffiarts/FoundryVTT-sheet-locker/releases/download/latest/README.md",
  "changelog": "https://github.com/coffiarts/FoundryVTT-sheet-locker/releases/download/latest/CHANGELOG.md",
  "compatibility": {
    "minimum": "11",
    "verified": "11",
    "maximum": "11"
  },
  "media": [
    {
      "type": "setup",
      "url": "modules/sheet-locker/artwork/user-lock-solid.svg",
      "thumbnail": "modules/sheet-locker/artwork/user-lock-solid.svg",
      "loop": false,
      "flags": {}
    }
  ],
  "authors": [
    {
      "name": "coffiarts"
    }
  ],
  "relationships": {
    "requires": [
      {
        "id": "socketlib",
        "type": "module",
        "manifest": "https://raw.githubusercontent.com/manuelVo/foundryvtt-socketlib/master/module.json",
        "compatibility": {
          "verified": "1.0.13"
        }
      }
    ]
  },
  "esmodules": [
    "/scripts/main.js",
    "/scripts/config.js",
    "/scripts/logger.js",
    "/scripts/chatinfo.js"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    },
    {
      "lang": "de",
      "name": "German",
      "path": "lang/de.json",
      "flags": {}
    }
  ],
  "packs": [
    {
      "name": "sheet-locker-macros",
      "label": "Sheet Locker - Macros",
      "path": "packs/sheet-locker-macros",
      "type": "Macro",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    }
  ],
  "socket": true
}